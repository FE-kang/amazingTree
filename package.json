{
  "name": "amazing-tree",
  "version": "1.2.4",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build:lib": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\" && set BUILD_LIB=true&& vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "replace-uuids": "node scripts/replace-uuids.mjs",
    "add-has-anim": "node scripts/add-has-anim.mjs"
  },
  "dependencies": {
    "vue": "^3.5.22",
    "vue-component-type-helpers": "^3.1.4"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass": "^1.94.1",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/amazing-tree.es.js",
      "require": "./dist/amazing-tree.cjs.js"
    },
    "./style.css": "./dist/amazing-tree.css",
    "./dist/*": "./dist/*"
  },
  "sideEffects": [
    "dist/*.css"
  ],
  "author": "Maxkang",
  "style": "./dist/amazing-tree.css"
}
